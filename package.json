{
  "name": "nabu",
  "version": "3.0.1",
  "description": "File QC DB",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "start:dev": "nodemon app.js",
    "start:prod": "NODE_ENV=production node --abort-on-uncaught-exception app.js",
    "test": "mocha --exit",
    "lint": "eslint .",
    "pre-commit": "npm test && lint-staged",
    "fw:clean": "docker run --rm -v $(pwd)/conf:/flyway/conf -v $(pwd)/sql:/flyway/sql --network=host flyway/flyway clean",
    "fw:migrate": "docker run --rm -v $(pwd)/conf:/flyway/conf -v $(pwd)/sql:/flyway/sql --network=host flyway/flyway migrate",
    "fw:test-clean": "docker run --rm -v $(pwd)/test:/flyway/conf -v $(pwd)/test/migrations:/flyway/sql --network=host flyway/flyway -cleanDisabled=false clean",
    "fw:test-migrate": "docker run --rm -v $(pwd)/test:/flyway/conf -v $(pwd)/test/migrations:/flyway/sql --network=host flyway/flyway migrate",
    "pretest": ". test/setup_test_dbs.sh",
    "posttest": ". test/stop_test_dbs.sh"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --write",
      "eslint --fix ."
    ]
  },
  "repository": {
    "type": "git"
  },
  "author": "Heather Armstrong",
  "license": "MIT",
  "dependencies": {
    "JSONStream": "^1.3.4",
    "activedirectory2": "^2.1.0",
    "body-parser": "^1.20.1",
    "compression": "^1.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^4.4.1",
    "moment": "^2.29.1",
    "node-flywaydb": "3.0.7",
    "pg-promise": "^11.4.3",
    "pg-query-stream": "^4.4.0",
    "prom-client": "^14.1.1",
    "serve-favicon": "^2.5.0",
    "sqlite3": "^5.1.5",
    "swagger-ui-express": "^4.6.2",
    "uid": "^2.0.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-exclude": "^2.1.0",
    "chai-http": "^4.3.0",
    "eslint": "^8.34.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "mocha": "10.2.0",
    "node-cmd": "^5.0.0",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.7",
    "rewire": "^6.0.0"
  }
}
