{
  "name": "nabu",
  "version": "2.1.0",
  "description": "File QC DB",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "start:dev": "nodemon app.js",
    "start:prod": "NODE_ENV=production nodejs --abort-on-uncaught-exception app.js",
    "test": "PORT=3001 SQLITE_LOCATION=$(pwd)/test DB_CONNECTION=postgres://test:test@localhost/qcdb_test LOG_LEVEL=test mocha --exit",
    "lint": "eslint .",
    "precommit": "lint-staged",
    "fw:clean": "docker run --rm -v $(pwd)/conf:/flyway/conf -v $(pwd)/sql:/flyway/sql --network=host boxfuse/flyway clean",
    "fw:migrate": "docker run --rm -v $(pwd)/conf:/flyway/conf -v $(pwd)/sql:/flyway/sql --network=host boxfuse/flyway migrate",
    "pretest": ". test/setup_test_dbs.sh"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --write",
      "eslint --fix .",
      "git add"
    ]
  },
  "repository": {
    "type": "git"
  },
  "author": "Heather Armstrong",
  "license": "MIT",
  "dependencies": {
    "JSONStream": "^1.3.4",
    "activedirectory2": "^2.1.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "moment": "^2.29.1",
    "pg-promise": "^10.10.1",
    "pg-query-stream": "^4.1.0",
    "prom-client": "^13.1.0",
    "serve-favicon": "^2.5.0",
    "sqlite3": "^5.0.2",
    "swagger-ui-express": "^4.1.6",
    "uid": "^2.0.0",
    "winston": "2.4.0"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-exclude": "^2.0.3",
    "chai-http": "^4.3.0",
    "eslint": "^7.24.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "mocha": "^8.3.2",
    "node-cmd": "^4.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "rewire": "^5.0.0"
  }
}
